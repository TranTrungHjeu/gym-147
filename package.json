{
  "name": "gym-147",
  "private": true,
  "workspaces": [
    "apps/web-admin",
    "services/*",
    "infrastructure/gateway/*",
    "packages/*"
  ],
  "scripts": {
    "setup": "bash scripts/setup.sh",
    "dev": "bash scripts/dev.sh",
    "dev:admin": "npm run dev -w apps/web-admin",
    "dev:member": "npm run dev -w services/member-service",
    "dev:identity": "npm run dev -w services/identity-service",
    "dev:schedule": "npm run dev -w services/schedule-service",
    "dev:billing": "npm run dev -w services/billing-service",
    "start:all": "concurrently \"npm run dev:member\" \"npm run dev:identity\" \"npm run dev:schedule\" \"npm run dev:billing\"",
    "build:shared": "npm run build --workspace=@gym-147/shared-types && npm run build --workspace=@gym-147/shared-utils && npm run build --workspace=@gym-147/shared-config && npm run build --workspace=@gym-147/shared-middleware",
    "build:all": "npm run build:shared && npm run build --workspaces",
    "deploy": "bash scripts/deploy.sh",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "docker:up": "cd infrastructure/docker && docker-compose up -d",
    "docker:down": "cd infrastructure/docker && docker-compose down",
    "docker:logs": "cd infrastructure/docker && docker-compose logs -f",
    "docker:build": "cd infrastructure/docker && docker-compose build"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5"
  }
}
