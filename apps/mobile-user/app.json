{
  "expo": {
    "name": "Gym147",
    "slug": "gym147-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "gym147",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gym147.mobile",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSMotionUsageDescription": "We need access to motion sensors to track your workout activity and calculate accurate calories."
      }
    },
    "android": {
      "package": "com.gym147.mobile",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ],
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "API_BASE_URL": "",
      "APP_NAME": "Gym147",
      "APP_VERSION": "1.0.0",
      "ENVIRONMENT": "development",
      "YOUTUBE_API_KEY": "",
      "router": {},
      "eas": {
        "projectId": "3f7fa9f0-c61b-4369-a910-4842643b6f30"
      }
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#000000",
          "sounds": ["default"],
          "mode": "production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#000000",
      "iosDisplayInForeground": true
    },
    "owner": "trantrunghjeu"
  }
}
