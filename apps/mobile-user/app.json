{
  "expo": {
    "name": "Gym147",
    "slug": "gym147-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "gym147",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gym147.mobile",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.gym147.mobile",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "APP_NAME": "Gym147",
      "APP_VERSION": "1.0.0",
      "ENVIRONMENT": "development",
      "YOUTUBE_API_KEY": "AIzaSyCjBveZYEH951NlmM7HaKpTqXZxQU3lTLg",
      "router": {},
      "eas": {
        "projectId": "3f7fa9f0-c61b-4369-a910-4842643b6f30"
      }
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      "expo-router",
      "expo-web-browser",
      "expo-notifications"
    ],
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#000000",
      "iosDisplayInForeground": true
    },
    "owner": "trantrunghjeu"
  }
}
